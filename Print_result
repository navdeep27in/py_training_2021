a=int(input("Enter 1st sub: "))
b=int(input("Enter 2nd sub: "))
c=int(input("Enter 3rd sub: "))
d=int(input("Enter 4th sub: "))
e=int(input("Enter 5th sub: "))

x=(a+b+c+d+e)*100/500

print ("Percentage =",x)

if x<33:
    print ("Result = Fail")
elif 33<x and x<60:
    print ("Result = Pass")
else:
    print ("Result = Merit")
